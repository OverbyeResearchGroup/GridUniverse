{
  "name": "gridverse",
  "version": "1.1.0",
  "author": "Zeyu Mao",
  "description": "The desktop application version of Gridverse",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "node --max_old_space_size=4096 node_modules/@vue/cli-service/bin/vue-cli-service.js build --open",
    "python:build": "pyinstaller --noconfirm --onefile --console --add-data ./py/datafields.json --distpath ./pydist ./py/ds_client.py",
    "electron:build": "node --max_old_space_size=4096 node_modules/@vue/cli-service/bin/vue-cli-service.js electron:build",
    "electron:serve": "vue-cli-service electron:serve",
    "electron:generate-icons": "electron-icon-builder --input=./public/icon.png --output=build --flatten",
    "preinstall": "npx npm-force-resolutions",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps"
  },
  "main": "background.js",
  "dependencies": {
    "@deck.gl/core": "^8.3.7",
    "@deck.gl/layers": "^8.3.7",
    "@luma.gl/core": "^8.3.1",
