{
  "name": "powerweb-electron",
  "version": "1.0.0",
  "author": "Zeyu Mao",
  "description": "The desktop application version of PowerWeb",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "node --max_old_space_size=4096 node_modules/@vue/cli-service/bin/vue-cli-service.js build --open",
    "python:build": "pyinstaller ./py/ds_client.py --clean --distpath ./pydist",
    "pack:build": "./node_modules/.bin/electron-packager . --overwrite --ignore=py$",
    "electron:build": "node --max_old_space_size=4096 node_modules/@vue/cli-service/bin/vue-cli-service.js electron:build",
    "electron:serve": "vue-cli-service electron:serve",
    "electron:generate-icons": "electron-icon-builder --input=./public/icon.png --output=build --flatten",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps"
  },
  "main": "background.js",
  "dependencies": {
    "@msgpack/msgpack": "^1.12.2",
    "@msrvida/sanddance": "^1.6.7",
    "@supermap/iclient-leaflet": "^10.2.0",
    "axios": "^0.19.0",
    "babel-loader": "^8.2.3",
    "clusters": "0.0.4",
    "convex-hull": "^1.0.3",
    "core-js": "^3.19.1",
    "echarts-gl": "^1.1.1",
    "echarts-leaflet": "^1.1.0",
    "echartslayer": "^0.1.6",
    "element-ui": "^2.15.6",
    "euclidean-distance": "^1.0.0",
    "fingerprintjs2": "^1.8.6",
    "geojson": "^0.5.0",
    "izitoast": "^1.4.0",
    "javascript-time-ago": "^1.0.35",
    "ky": "^0.4.1",
    "leaflet.sync": "^0.2.4",
    "lodash": "^4.17.21",
    "luma.gl": "^6.4.3",
    "mapbox-gl": "^0.47.0",
    "mathjs": "^5.10.3",
    "mqtt": "^2.18.8",
    "numeral": "^2.0.6",
    "papaparse": "^4.6.3",
    "push.js": "^1.0.9",
    "python-shell": "^1.0.8",
    "register-service-worker": "^1.7.2",
    "vis": "^4.21.0",
    "vue": "^2.6.14",
    "vue-charts": "^0.2.1",
    "vue-drag-zone": "^2.0.0-rc1",
    "vue-echarts": "^4.0.4",
    "vue-grid-layout": "^2.3.12",
    "vue-introjs": "^1.3.2",
    "vue-marquee-text-component": "^1.1.1",
    "vue-router": "^3.5.3",
    "vue-spinkit": "^1.7.10",
    "vue-status-indicator": "^1.2.1",
    "vuetify": "^1.5.20",
    "vuex": "^3.6.2",
    "webkul-micron": "^1.1.6",
    "zeromq": "^6.0.0-beta.6"
  },
  "devDependencies": {
    "@mdi/font": "^2.5.94",
    "@vue/cli-plugin-babel": "^4.5.15",
    "@vue/cli-plugin-router": "^4.5.15",
    "@vue/cli-plugin-vuex": "^4.5.15",
    "@vue/cli-service": "^4.5.15",
    "cross-env": "^6.0.3",
    "echarts": "^4.7.0",
    "electron": "^6.0.0",
    "electron-icon-builder": "^1.0.2",
    "electron-packager": "^14.2.1",
    "element-theme-dark": "^1.0.3",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "vue-cli-plugin-electron-builder": "^1.4.6",
    "vue-template-compiler": "^2.6.14"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
